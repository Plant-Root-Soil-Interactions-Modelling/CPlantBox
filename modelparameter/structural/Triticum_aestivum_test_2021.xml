<plant>
    <root name="taproot" subType="1">
        <parameter name="gf" value="3"/>
        <!--Growth function number [1]-->
        <parameter name="tropismT" value="1"/>
        <!--Type of root tropism (plagio = 0, gravi = 1, exo = 2, hydro, chemo = 3)-->
        <parameter name="a" value="0.050000001"/>
        <!--radius [cm]-->
        <parameter name="colorB" value="0.2"/>
        <!--Root color, blue component [0.-1.]-->
        <parameter name="colorG" value="0.2"/>
        <!--Root color, green component [0.-1.]-->
        <parameter name="colorR" value="0.60000002"/>
        <!--Root color, red component [0.-1.]-->
        <parameter name="dx" value="3"/>
        <!--Axial resolution [cm] (maximal segment size)-->
        <parameter name="dxMin" value="1"/>
        <!--Axial resolution [cm] (minimal segment size)-->
        <parameter name="la" value="12"/>
        <!--Apical zone [cm]-->
        <parameter name="lb" value="0.80000001"/>
        <!--Basal zone [cm]-->
        <parameter name="ldelay" value="4"/>
        <!--Lateral root emergence delay [day]-->
        <parameter name="lmax" value="60"/>
        <!--Maximal root length [cm]-->
        <parameter name="ln" value="0.40000001"/>
        <!--Inter-lateral distance [cm]-->
        <parameter name="lnk" value="0"/>
        <!--Slope of inter-lateral distances [1]-->
        <parameter name="r" value="2.4000001"/>
        <!--Initial growth rate [cm day-1]-->
        <parameter name="rlt" value="1e+09"/>
        <!--Root life time [day]-->
        <parameter name="theta" value="0.40000001"/>
        <!--Angle between root and parent root [rad]-->
        <parameter name="tropismN" value="0.5"/>
        <!--Number of trials of root tropism-->
        <parameter name="tropismS" value="0.26179901"/>
        <!--Mean value of expected change of root tropism [1/cm]-->
        <parameter name="successor" ruleId="0" numLat="1" Where="" subType="2" organType="2" percentage="1"/>
        <!--Sub type of lateral -->
    </root>
    <root name="lateral1" subType="2">
        <parameter name="gf" value="3"/>
        <!--Growth function number [1]-->
        <parameter name="tropismT" value="1"/>
        <!--Type of root tropism (plagio = 0, gravi = 1, exo = 2, hydro, chemo = 3)-->
        <parameter name="a" value="0.02"/>
        <!--radius [cm]-->
        <parameter name="colorB" value="0.2"/>
        <!--Root color, blue component [0.-1.]-->
        <parameter name="colorG" value="0.2"/>
        <!--Root color, green component [0.-1.]-->
        <parameter name="colorR" value="0.60000002"/>
        <!--Root color, red component [0.-1.]-->
        <parameter name="dx" value="3"/>
        <!--Axial resolution [cm] (maximal segment size)-->
        <parameter name="dxMin" value="1"/>
        <!--Axial resolution [cm] (minimal segment size)-->
        <parameter name="la" value="5"/>
        <!--Apical zone [cm]-->
        <parameter name="lb" value="1"/>
        <!--Basal zone [cm]-->
        <parameter name="ldelay" value="5"/>
        <!--Lateral root emergence delay [day]-->
        <parameter name="lmax" value="10"/>
        <!--Maximal root length [cm]-->
        <parameter name="ln" value="0.25"/>
        <!--Inter-lateral distance [cm]-->
        <parameter name="lnk" value="0"/>
        <!--Slope of inter-lateral distances [1]-->
        <parameter name="r" value="1"/>
        <!--Initial growth rate [cm day-1]-->
        <parameter name="rlt" value="1e+09"/>
        <!--Root life time [day]-->
        <parameter name="theta" value="0.79000002"/>
        <!--Angle between root and parent root [rad]-->
        <parameter name="tropismN" value="1"/>
        <!--Number of trials of root tropism-->
        <parameter name="tropismS" value="0.5"/>
        <!--Mean value of expected change of root tropism [1/cm]-->
        <parameter name="successor" ruleId="0" numLat="1" Where="" subType="3" organType="2" percentage="1"/>
        <!--Sub type of lateral -->
    </root>
    <root name="lateral2" subType="3">
        <parameter name="gf" value="3"/>
        <!--Growth function number [1]-->
        <parameter name="tropismT" value="1"/>
        <!--Type of root tropism (plagio = 0, gravi = 1, exo = 2, hydro, chemo = 3)-->
        <parameter name="a" value="0.02"/>
        <!--radius [cm]-->
        <parameter name="colorB" value="0.2"/>
        <!--Root color, blue component [0.-1.]-->
        <parameter name="colorG" value="0.2"/>
        <!--Root color, green component [0.-1.]-->
        <parameter name="colorR" value="0.60000002"/>
        <!--Root color, red component [0.-1.]-->
        <parameter name="dx" value="3"/>
        <!--Axial resolution [cm] (maximal segment size)-->
        <parameter name="dxMin" value="1"/>
        <!--Axial resolution [cm] (minimal segment size)-->
        <parameter name="la" value="3"/>
        <!--Apical zone [cm]-->
        <parameter name="lb" value="1"/>
        <!--Basal zone [cm]-->
        <parameter name="ldelay" value="-1"/>
        <!--Lateral root emergence delay [day]-->
        <parameter name="lmax" value="4"/>
        <!--Maximal root length [cm]-->
        <parameter name="ln" value="1"/>
        <!--Inter-lateral distance [cm]-->
        <parameter name="lnk" value="0"/>
        <!--Slope of inter-lateral distances [1]-->
        <parameter name="r" value="0.60000002"/>
        <!--Initial growth rate [cm day-1]-->
        <parameter name="rlt" value="1e+09"/>
        <!--Root life time [day]-->
        <parameter name="theta" value="0.95993102"/>
        <!--Angle between root and parent root [rad]-->
        <parameter name="tropismN" value="1"/>
        <!--Number of trials of root tropism-->
        <parameter name="tropismS" value="0.40000001"/>
        <!--Mean value of expected change of root tropism [1/cm]-->
    </root>
    <stem name="mainstem" subType="1">
        <parameter name="gf" value="3"/>
        <!--Growth function number [1]-->
        <parameter name="lnf" value="0"/>
        <!--Type of inter-branching distance (0 homogeneous, 1 linear inc, 2 linear dec, 3 exp inc, 4 exp dec)-->
        <parameter name="nodalGrowth" value="0"/>
        <!--nodal growth function (sequential = 0, equal = 0)-->
        <parameter name="tropismT" value="1"/>
        <!--Type of stem tropism (plagio = 0, gravi = 1, exo = 2, hydro, chemo = 3)-->
        <parameter name="BetaDev" value="0.2"/>
        <!--RevRotation deviation-->
        <parameter name="InitBeta" value="0.2"/>
        <!--Initial RevRotation-->
        <parameter name="RotBeta" value="0.60000002"/>
        <!--RevRotation of the stem-->
        <parameter name="a" value="0.15000001"/>
        <!--Stem radius [cm]-->
        <parameter name="delayNGEnd" value="24"/>
        <!--delay between stem creation and start of nodal growth-->
        <parameter name="delayNGStart" value="4"/>
        <!--delay between stem creation and start of nodal growth-->
        <parameter name="dx" value="1"/>
        <!--Axial resolution [cm] (maximal segment size)-->
        <parameter name="dxMin" value="0.1"/>
        <!--Axial resolution [cm] (minimal segment size)-->
        <parameter name="la" value="0"/>
        <!--Apical zone [cm]-->
        <parameter name="lb" value="1"/>
        <!--Basal zone [cm]-->
        <parameter name="ldelay" value="10"/>
        <!--delay between latteral creation and start of nodal growth-->
        <parameter name="lmax" value="35"/>
        <!--Maximal stem length [cm]-->
        <parameter name="ln" value="8.75"/>
        <!--Inter-lateral distance [cm]-->
        <parameter name="r" value="1"/>
        <!--Initial growth rate [cm day-1]-->
        <parameter name="rlt" value="1e+09"/>
        <!--Stem life time [day]-->
        <parameter name="theta" value="0.1"/>
        <!--Angle between stem and parent stem [rad]-->
        <parameter name="tropismAge" value="0"/>
        <!--Age at which organ switch tropism-->
        <parameter name="tropismN" value="1"/>
        <!--Number of trials of stem tropism-->
        <parameter name="tropismS" value="0.0099999998"/>
        <!--Mean value of expected change of stem tropism [1/cm]-->
        <parameter name="successor" ruleId="0" numLat="1" Where="" subType="1" organType="4" percentage="1"/>
        <!--Sub type of lateral -->
    </stem>
    <leaf name="lateral1" subType="1">
        <parameter name="geometryN" value="100"/>
        <!--leaf geometry resolution-->
        <parameter name="gf" value="3"/>
        <!--Growth function number [1]-->
        <parameter name="isPseudostem" value="1"/>
        <!--Do the leaf sheaths make a pseudostem?-->
        <parameter name="lnf" value="0"/>
        <!--Type of inter-branching distance (0 homogeneous, 1 linear inc, 2 linear dec, 3 exp inc, 4 exp dec)-->
        <parameter name="parametrisationType" value="1"/>
        <!--Leaf geometry parametrisation type-->
        <parameter name="shapeType" value="1"/>
        <!--leaf shape type for definition of area and volume (cylinder = 0, cuboid = 1, user-defined 2D shape= 2)-->
        <parameter name="tropismT" value="1"/>
        <!--Type of leaf tropism (plagio = 0, gravi = 1, exo = 2, hydro, chemo = 3)-->
        <parameter name="BetaDev" value="0.2"/>
        <!--RevRotation deviation-->
        <parameter name="InitBeta" value="0.2"/>
        <!--Initial RevRotation-->
        <parameter name="RotBeta" value="0.60000002"/>
        <!--RevRotation of the leaf-->
        <parameter name="Width_blade" value="0.89999998"/>
        <!--width blade [cm]-->
        <parameter name="Width_petiole" value="0.34999999"/>
        <!--width petiole [cm]-->
        <parameter name="a" value="0.029999999"/>
        <!--radius [cm]-->
        <parameter name="areaMax" value="35"/>
        <!--Maximal leaf area [cm2]-->
        <parameter name="dx" value="2"/>
        <!--Axial resolution [cm] (maximal segment size)-->
        <parameter name="dxMin" value="1"/>
        <!--Axial resolution [cm] (minimal segment size)-->
        <parameter name="la" value="35"/>
        <!--Apical zone [cm]-->
        <parameter name="lb" value="8"/>
        <!--Basal zone [cm]-->
        <parameter name="ldelay" value="-1"/>
        <!--Lateral emergence delay [day]-->
        <parameter name="lmax" value="43"/>
        <!--Maximal leaf length [cm]-->
        <parameter name="ln" value="0"/>
        <!--Inter-lateral distance [cm]-->
        <parameter name="r" value="4"/>
        <!--Initial growth rate [cm day-1]-->
        <parameter name="rlt" value="1e+09"/>
        <!--Leaf life time [day]-->
        <parameter name="theta" value="0.2"/>
        <!--Angle between leaf and parent leaf [rad]-->
        <parameter name="tropismAge" value="0"/>
        <!--Age at which organ switch tropism-->
        <parameter name="tropismN" value="18"/>
        <!--Number of trials of leaf tropism-->
        <parameter name="tropismS" value="0.02"/>
        <!--Mean value of expected change of leaf tropism [1/cm]-->
        <parameter name="leafGeometry" phi="-3" x="1"/>
        <!--Leaf geometry parametrisation parameter-->
        <parameter name="leafGeometry" phi="-2.1000000000000001" x="1"/>
        <!--Leaf geometry parametrisation parameter-->
        <parameter name="leafGeometry" phi="0" x="1"/>
        <!--Leaf geometry parametrisation parameter-->
        <parameter name="leafGeometry" phi="2.4500000000000002" x="1"/>
        <!--Leaf geometry parametrisation parameter-->
        <parameter name="leafGeometry" phi="3.5" x="1"/>
        <!--Leaf geometry parametrisation parameter-->
    </leaf>
</plant>
