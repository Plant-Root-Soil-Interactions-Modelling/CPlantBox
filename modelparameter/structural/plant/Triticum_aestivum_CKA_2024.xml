<?xml version="1.0" encoding="UTF-8"?>
<plant name="Selhausen" filetype="parameters">
<organ type="seed" name="true" subType="0">
        <parameter name="maxTi" value="4"/>
        <!--Maximal number of tillers [1]-->
        <!--The total number of crown roots will depend on the number of tillers and the number of roots per tiller (see organ "stem")-->
        <parameter name="firstTi" value="10" />
        <!--Emergence of first tiller [day]-->
        <parameter name="delayTi" value="6" />
        <!--Delay between tiller appearance [day]-->
        <parameter name="delayB" value="3"/>
        <!--Time delay between the basal roots [day]-->
        <parameter name="firstB" value="1"/>
        <!--Emergence of first basal root [day]-->
        <parameter name="maxB" value="6"/>
        <!--Maximal number of basal roots [1]-->
        <parameter name="seedPos.x" value="0"/>
        <!--X-Coordinate of seed position [cm]-->
        <parameter name="seedPos.y" value="0"/>
        <!--Y-Coordinate of seed position [cm]-->
        <parameter name="seedPos.z" value="-.3"/>
        <!--Z-Coordinate of seed position [cm]-->
        <parameter name="delayDefinition" value="1"/>
        <!--Definition of growth delay: distance = 0, time = 1-->
		
</organ>
<organ type="root" name="primary" subType="1">
        <parameter name="gf" value="1"/>
        <!--Growth function number [1]-->
        <parameter name="tropismT" value="1"/>
        <!--Type of root tropism (plagio = 0, gravi = 1, exo = 2, hydro, chemo = 3)-->
        <parameter name="a" value="0.015" dev="0"/>
        <!--Root radius [cm]-->
        <parameter name="colorB" value="0.156863"/>
        <!--Root color, blue component [0.-1.]-->
        <parameter name="colorG" value="0.49019599"/>
        <!--Root color, green component [0.-1.]-->
        <parameter name="colorR" value="0.54901999"/>
        <!--Root color, red component [0.-1.]-->
        <parameter name="dx" value="1"/>
        <!--Axial resolution [cm] (maximal segment size)-->
        <parameter name="la" value="40" dev="0"/>
         <!--Apical zone [cm]-->
        <parameter name="lb" value="6.5" dev="0"/>
        <!--Basal zone [cm]-->
        <parameter name="lmax" value="120" dev="0"/>
        <!--Maximal root length [cm]-->
        <parameter name="ln" value="0.75" dev="0"/>
        <!--Inter-lateral distance [cm]-->
        <parameter name="r" value=".8" dev="0"/>
        <!--Initial growth rate [cm day-1]-->
        <parameter name="rlt" value="1e+09" dev="1e+08"/>
        <!--Root life time [day]-->
        <parameter name="tropismN" value="1"/>
        <!--Number of trials of root tropism-->
        <parameter name="tropismS" value="0.30000001"/>
        <!--Mean value of expected change of root tropism [1/cm]-->
        <parameter name="successor" ruleId="0" subType="2" organType ="2" percentage="1"/>
</organ>
<organ type="root" name="basal" subType="4">
        <parameter name="gf" value="1"/>
        <!--Growth function number [1]-->
        <parameter name="tropismT" value="1"/>
        <!--Type of root tropism (plagio = 0, gravi = 1, exo = 2, hydro, chemo = 3)-->
        <parameter name="a" value="0.015" dev=""/>
        <!--Root radius [cm]-->
        <parameter name="colorB" value="0.156863"/>
        <!--Root color, blue component [0.-1.]-->
        <parameter name="colorG" value="0.49019599"/>
        <!--Root color, green component [0.-1.]-->
        <parameter name="colorR" value="0.54901999"/>
        <!--Root color, red component [0.-1.]-->
        <parameter name="dx" value="1"/>
        <!--Axial resolution [cm] (maximal segment size)-->
        <parameter name="la" value="40" dev="0"/>
         <!--Apical zone [cm]-->
        <parameter name="lb" value="6.5" dev="0"/>
        <!--Basal zone [cm]-->
        <parameter name="lmax" value="120" dev="0"/>
        <!--Maximal root length [cm]-->
        <parameter name="ln" value="0.95" dev="0"/>
        <!--Inter-lateral distance [cm]-->
        <parameter name="r" value=".8" dev="0"/>
        <!--Initial growth rate [cm day-1]-->
        <parameter name="rlt" value="1e+09" dev="1e+08"/>
        <!--Root life time [day]-->
        <parameter name="tropismN" value="1"/>
        <!--Number of trials of root tropism-->
        <parameter name="tropismS" value="0.27"/>
        <!--Mean value of expected change of root tropism [1/cm]-->
        <parameter name="successor" ruleId="0" subType="2" organType ="2" percentage="1"/>
</organ>
<organ type="root" name="crown" subType="5">
        <parameter name="gf" value="1"/>
        <!--Growth function number [1]-->
        <parameter name="tropismT" value="1"/>
        <!--Type of root tropism (plagio = 0, gravi = 1, exo = 2, hydro, chemo = 3)-->
        <parameter name="a" value="0.03" dev="0"/>
        <!--Root radius [cm]-->
        <parameter name="colorB" value="0.156863"/>
        <!--Root color, blue component [0.-1.]-->
        <parameter name="colorG" value="0.49019599"/>
        <!--Root color, green component [0.-1.]-->
        <parameter name="colorR" value="0.54901999"/>
        <!--Root color, red component [0.-1.]-->
        <parameter name="dx" value="2"/>
        <!--Axial resolution [cm] (maximal segment size)-->
        <parameter name="la" value="40" dev="0"/>
        <!--Apical zone [cm]-->
        <parameter name="lb" value="6.5" dev="0"/>
        <!--Basal zone [cm]-->
        <parameter name="lmax" value="100" dev="0."/>
        <!--Maximal root length [cm]-->
        <parameter name="ln" value="0.95" dev="0"/>
        <!--Inter-lateral distance [cm]-->
        <parameter name="r" value="1.15" dev="0"/>
        <!--Initial growth rate [cm day-1]-->
        <parameter name="rlt" value="1e+09" dev="1e+08"/>
        <!--Root life time [day]-->
        <parameter name="tropismN" value="1"/>
        <!--Number of trials of root tropism-->
        <parameter name="tropismS" value="0.27"/>
        <!--Mean value of expected change of root tropism [1/cm]-->
        <parameter name="successor" ruleId="0" subType="2" organType ="2" percentage="1"/>
</organ>
<organ type="root" name="lateral1" subType="2">
        <parameter name="gf" value="1"/>
        <!--Growth function number [1]-->
        <parameter name="tropismT" value="1"/>
        <!--Type of root tropism (plagio = 0, gravi = 1, exo = 2, hydro, chemo = 3)-->
        <parameter name="a" value="0.01" dev="0"/>
        <!--Root radius [cm]-->
        <parameter name="colorB" value="0.156863"/>
        <!--Root color, blue component [0.-1.]-->
        <parameter name="colorG" value="0.49019599"/>
        <!--Root color, green component [0.-1.]-->
        <parameter name="colorR" value="0.54901999"/>
        <!--Root color, red component [0.-1.]-->
        <parameter name="dx" value="1"/>
        <!--Axial resolution [cm] (maximal segment size)-->
        <parameter name="la" value="2" dev="0"/>
        <parameter name="lb" value="1" dev="0"/>
        <!--Basal zone [cm]-->
        <parameter name="lmax" value="5" dev="0"/>
        <!--Maximal root length [cm]-->
        <parameter name="ln" value="0.6" dev="0"/>
        <!--Inter-lateral distance [cm]-->
        <parameter name="r" value="0.15" dev="0"/>
        <!--Initial growth rate [cm day-1]-->
        <parameter name="rlt" value="1e+09" dev="1e+08"/>
        <!--Root life time [day]-->
        <parameter name="theta" value="1.6" dev="0"/>
        <!--Angle between root and parent root [rad]-->
        <parameter name="tropismN" value="1"/>
        <!--Number of trials of root tropism-->
        <parameter name="tropismS" value="0.5"/>
        <!--Mean value of expected change of root tropism [1/cm]-->
        <parameter name="successor" ruleId="0" subType="3" organType ="2" percentage="1"/>
</organ>
<organ type="root" name="lateral2" subType="3">
        <parameter name="gf" value="1"/>
        <!--Growth function number [1]-->
        <parameter name="tropismT" value="1"/>
        <!--Type of root tropism (plagio = 0, gravi = 1, exo = 2, hydro, chemo = 3)-->
        <parameter name="a" value="0.01" dev=""/>
        <!--Root radius [cm]-->
        <parameter name="colorB" value="0.156863"/>
        <!--Root color, blue component [0.-1.]-->
        <parameter name="colorG" value="0.49019599"/>
        <!--Root color, green component [0.-1.]-->
        <parameter name="colorR" value="0.54901999"/>
        <!--Root color, red component [0.-1.]-->
        <parameter name="dx" value="1"/>
        <!--Axial resolution [cm] (maximal segment size)-->
        <parameter name="la" value="3" dev="0"/>
        <!--Apical zone [cm]-->
        <parameter name="lb" value="1" dev="0"/>
        <!--Basal zone [cm]-->
        <parameter name="lmax" value="4" dev="0"/>
        <!--Maximal root length [cm]-->
        <parameter name="ln" value="0" dev="0"/>
        <!--Inter-lateral distance [cm]-->
        <parameter name="r" value="0.15" dev="0"/>
        <!--Initial growth rate [cm day-1]-->
        <parameter name="rlt" value="1e+09" dev="1e+08"/>
        <!--Root life time [day]-->
        <parameter name="theta" value="0.96" dev="0"/>
        <!--Angle between root and parent root [rad]-->
        <parameter name="tropismN" value="1"/>
        <!--Number of trials of root tropism-->
        <parameter name="tropismS" value="0.1"/>
        <!--Mean value of expected change of root tropism [1/cm]-->
</organ>
<organ type="stem" name="mainstem" subType="1">
	<parameter name="lb" value="2" />
	<!--Basal zone [cm] defines where node 0 is located above the seed; also, it is needed for parametrizing the internodal distance before stem elongation starts-->
	<parameter name="la" value="0" />
	<!--Apical zone [cm] defines the location of the last node -> must be 0 so that the flag leaf grows above this last node-->
	<parameter name="ln" value="9" />
	<!--Inter-lateral distance [cm] defines the max. internodal distance (including stem elongation) and thus the muximum number of leaves;-->
	<parameter name="lnf" value="0" />
	<!--type of inter-branching distance (0 homogeneous, 1 linear inc, 2 linear dec, 3 exp inc, 4 exp dec)-->
	<parameter name="lmax" value="63" />
	<!--maximal stem length [cm]-->
	<parameter name="r" value="1.5" />
	<!--Initial growth rate [cm/day]-->
	<parameter name="ldelay" value="6" />
	<!--growth delay of laterals [day] (i.e. of the developing leaves, should be defined according to the phyllocron)-->
	<parameter name="nodalGrowth" value="1" />
	<!--wheter to implement internodal growth (0 = not implemented, 1 = implemented)-->
	<parameter name="delayNGStart" value="6"  />
	<!--time span between stem create and growth stop [day]-->
	<parameter name="delayNGEnd" value="55" />
	<!--time span between stem create and stem growth reinitiation (i.e. after growth pause = delayNGEnd - delayNGStart) [day]-->
	<parameter name="a" value="0.35" dev="0.0" />
	<!--Stem radius [cm]-->
	<parameter name="tropismT" value="1"/>
	<!--Type of tropism; actually irrelevant, as long as tropismS is set very low -->
	<parameter name="tropismN" value="1"/>
	<!--Number of trials of tropism-->
	<parameter name="tropismS" value="0.01"/>
	<!--Mean value of expected change of tropism [1/cm]-> set very low, to secure that stem grows erect-->
	<parameter name="dx" value="1"/>
	<parameter name="theta" value=".15" dev="0"/>
	<!--Angle between stem and parent stem-->
	<parameter name="rlt" value="1000000000" dev="0"/>
	<!--Stem life time [day]-->
	<parameter name="gf" value="2"/>
	!--Growth function (1=negative exponential, 2=linear, 3 exponential with carbon/water limitation)-->
	<parameter name="successor" ruleId="0" numLat="1" subtype="2" where="-0" percentage="1"/> 
	<!--subtype 2= leaf, where -0 indicates that there are no leaves in the node 0 of the main stem; model automatically assumes that the organ is a leaf-->
	<parameter name="successor" ruleId="1" numLat="2" subType="5" organType ="2"  where="0" percentage="1"/>
	<!--subtype 5= crown root, organType 2 = root; where 0 indicates that crown roots only grow on the node 0 (i.e. the stem base)-->	
</organ>
<organ type="leaf" name="lateral1" subType="2" >
	<parameter name="lb" value="0" dev="0"/>
	<!--basal zone (sheath length) [cm] -> use 0 if the leaf arises diretcly from the (pseudo)stem;-->
	<parameter name="la" value="0" dev="0"/>
	<!--Apical zone (not really relevant for leaves) [cm];-->
	<parameter name="lmax" value="16" dev="0"/>
	<!--total maximal leaf length (blade + sheath) [cm];-->
	<parameter name="r" value="2" dev="0"/>
	<!--growth rate during exponential phase [cm];-->
    	<parameter name="a" value="0.04" dev="0"/>
    	<!--leaf thickness [cm];-->
    	<parameter name="Width_petiole" value="0.35" />
    	<!--sheat width [cm];-->
    	<parameter name="Width_blade" value="1.4" />
    	<!--blade width [cm];-->
    	<parameter name="areaMax" value="20" />
    	<!--max leaf area [cm2] (should be smaller than lmax*Width_blade) ;-->
    	<parameter name="RotBeta" value="1"/>
    	<!--rotation angle between leaves [rad];-->
    	<parameter name="isPseudostem" value="1"/>
    	<!--do the leaf sheats make a pseudostem? (0 false, 1 true)-->
    	<parameter name="shapeType" value="2" />
    	<!--type of shape used for the leaves: cylinder = 0, cuboid = 1, shape_2D = 2. Select cuboid to be able to represent the thickness-->
    	<parameter name="tropismT" value="6"/>
    	<!--Tropism type: plagiotropism (tendency horizontal layer) = 0, gravitropism = 1,  exotropism (tendency keep initial heading) = 2; hydrotropism = 3-->
    	<!--Tropism type: combined tropisms = 4, twist tropism = 5, antigravitropism = 6 -->
    	<parameter name="tropismN" value="5"/>
    	<!--Number of trials for tropism -->
    	<parameter name="tropismS" value="0.15"/>
    	<!--Mean value of expected change in tropism -->
    	<parameter name="dx" value="1"/>
	<!--Maximal segment size [cm]-->
    	<parameter name="theta" value="0.15" />
    	<parameter name="rlt" value="1000000000" />
    	<parameter name="gf" value="3"/>
    	<parameter name="shapeType" value="2"/>
    	<!--Type of geometry. 0 = cylindrical, 1 = cuboid (i.e. 3d) and 2= 2d-shape. Defalut is 2,-->
    	<parameter name="parametrisationType" value="1"/>
    	<!--Value = 1: leaf geometry is described along the main axis. Alternative is radially (value = 0). For grasses this should always be 1 -->
	<parameter name="leafGeometry" phi="-3, -2.1, 0, 2.45, 3.5" x="1, 1, 1, 1, 1"/>
	<!--phy and x are coordinates of vortixes as somehow control the shape of the leaf (not sure how). changes must be quite large to affect leaf shape-->
	
	
</organ>
</plant>
