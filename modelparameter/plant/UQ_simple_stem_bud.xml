	<!--max = 80. here up to 21cm (20 days, 7 lats)-->
	<parameter name="lmax" value="67.5" dev="0"/>
	<!--3.5cm every 2 days, a bit higher because neg exp-->
	<parameter name="r" value="1.8" dev="0"/>
	<parameter name="a" value="0.5" dev="0.02"/>
	<parameter name="tropismT" value="0"/>
	<parameter name="tropismN" value="1"/>
	<parameter name="tropismS" value="0"/>
